@model AllUsersProductViewModel


<h2>All Product of Users !!!!</h2>
<!--@{
	//var reer = "~/images/" + Model.All.FirstOrDefault().ImagePath;

}-->
@if (!string.IsNullOrEmpty(ViewBag.Error))
{
	<div class="alert alert-danger alert-dismissible">
		<button type="button" class="closebtn" data-dismiss="alert" onclick="this.parentElement.style.display='flex';">&times;</button>
		<strong>Error!</strong> @ViewBag.Error
	</div>
}

<div class="search-container">
	<form  asp-action="SearchProduct" method="get" class="form-horizontal">
		<input type="text"  name="searchString">
		<!--<button type="submit"><i class="fa fa-search"></i></button>-->
		<button class="btn btn-primary" type="submit">Search</button>
	</form>
</div>
<br/>
<br/>

<div class="container-fluid">
	<div class="row">
		@foreach (var item in Model.All)
		{
			var path = "~/images/" + item.ImagePath;
			<div class="col-sm-4">
				<img class="card-image-left" src="@path" asp-append-version="true" style="width:250px;height:142px;" />
				<h4>@item.BrandName</h4>
				<h4>@item.NameOfModel</h4>
				<a asp-area="Users" asp-controller="Users" asp-action="Details" asp-route-id="@item.Id">@item.NameOfModel</a>
				<p class="card-text"><strong>Price</strong> - @item.Price</p>
				<p class="card-text"><strong>Size</strong> - @item.NumberOfSize</p>
				<p class="card-text">@item.Description</p>
				<p class="card-text">@item.Gender</p>
				<p class="card-text">@item.Color</p>

			</div>
		}

	</div>
</div>
<br />
