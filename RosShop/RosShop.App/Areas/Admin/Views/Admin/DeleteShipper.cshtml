@model int
@{
	ViewData["Title"] = "DeleteShipper";
}

<h2>Are you sure you want to delete this shipper ?</h2>

<a class="btn btn-success" asp-area="Admin" asp-controller="Admin" asp-action="DestroyShipper" asp-route-id="@Model">Confirm Delete</a>
<a class="btn btn-default" asp-area="Admin" asp-controller="Admin" asp-action="AllShipper">Back</a>